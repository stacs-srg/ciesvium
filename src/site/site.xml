<project xmlns="http://maven.apache.org/DECORATION/1.4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd" name="Ciesvium">
    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <custom>
        <reflowSkin>
            <!--Do not display the creator of skin-->
            <skinAttribution>false</skinAttribution>
            <protocolRelativeURLs>true</protocolRelativeURLs>
            <brand>
                <name>Ciesvium</name>
                <href>index.html</href>
            </brand>
            <navbarInverse>false</navbarInverse>
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <breadcrumbs>false</breadcrumbs>
        </reflowSkin>
    </custom>

    <body>

        <links>
            <item name="Home" href="index.html"/>
            <item name="Project Info" href="project-info.html"/>
            <item name="Install" href="install/"/>
            <item name="API" href="project-reports.html"/>
        </links>
    </body>

    <!--Show the last publish date at the bottom of the page-->
    <publishDate position="bottom" format="dd MMM yyyy"/>

    <!--Do not display the version-->
    <version position="none"/>
</project>
